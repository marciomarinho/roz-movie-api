# Example .env file for local development
# Copy this to .env and update values as needed

# Application
APP_NAME=Movie API
API_V1_PREFIX=/api

# PostgreSQL Database
DB_HOST=localhost
DB_PORT=5432
DB_NAME=postgres
DB_USER=postgres
DB_PASSWORD=mysecretpassword

# Authentication (optional - leave empty to disable)
# Uncomment the line below and set a value to require X-API-Key header on all requests
API_KEY=your-secret-api-key-here

# Logging
LOG_LEVEL=INFO
